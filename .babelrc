{
  "presets": [
    ["es2015", { "modules": false }],
    "react",
    "stage-2"
  ],
  "plugins": [
    ["transform-runtime", {
      "helpers": false,
      "polyfill": true,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }],
    "react-hot-loader/babel",
    "transform-decorators-legacy",
    ["import", { "libraryName": "antd", "style": "css" }]
  ]
}
